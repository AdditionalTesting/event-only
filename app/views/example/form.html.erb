example@form page

<%= form_for @job_application do |f| %>

<div>
  Full Name: <br>
  <%= f.text_field :full_name %>
</div>

<div>
  Phone: <br>
  <%= f.phone_field :phone %>
</div>

<div>
  Hobby: <br>
  <%= f.text_field :hobby %>
</div>

<div>
  Years Experience: <br>
  <%= f.number_field :years_experience %>
</div>

<div>
  Date: <br>
  <%= f.date_field :date %>
</div>

<% end %>
